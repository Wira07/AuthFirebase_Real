<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <activity
        android:id="@+id/loginActivity"
        android:name="com.wira_fkom.authfirebase_real.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />

    <activity
        android:id="@+id/registerActivity"
        android:name="com.wira_fkom.authfirebase_real.RegisterActivity"
        android:label="activity_register"
        tools:layout="@layout/activity_register" />

    <activity
        android:id="@+id/mainActivity"
        android:name="com.wira_fkom.authfirebase_real.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.wira_fkom.authfirebase_real.nav_fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/historyFragment"
        android:name="com.wira_fkom.authfirebase_real.bottom_fragment.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/cartFragment"
        android:name="com.wira_fkom.authfirebase_real.bottom_fragment.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/categoryFragment"
        android:name="com.wira_fkom.authfirebase_real.bottom_fragment.CategoryFragment"
        android:label="fragment_category"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.wira_fkom.authfirebase_real.bottom_fragment.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" />

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.wira_fkom.authfirebase_real.nav_fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/settingFragment"
        android:name="com.wira_fkom.authfirebase_real.nav_fragment.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />

</navigation>
